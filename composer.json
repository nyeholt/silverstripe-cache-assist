{
    "name": "nyeholt/silverstripe-cache-assist",
    "description": "Modules to assist with upstream caching",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
        "cache",
        "cloudflare"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "marcus@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\Cache\\": "src/",
            "Symbiote\\Cache\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
